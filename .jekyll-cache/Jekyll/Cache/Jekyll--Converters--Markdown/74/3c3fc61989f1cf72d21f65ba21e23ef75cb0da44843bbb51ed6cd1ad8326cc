<div class="language-plaintext highlighter-rouge"><div class="highlight"><pre class="highlight"><code>class Demo:
    def __enter__(self):
        print("enter function")
        return "Foo"

    def __exit__(self, exc_type, exc_val, exc_tb):
        print("exit")

with Demo() as demo:
    print("hello")
