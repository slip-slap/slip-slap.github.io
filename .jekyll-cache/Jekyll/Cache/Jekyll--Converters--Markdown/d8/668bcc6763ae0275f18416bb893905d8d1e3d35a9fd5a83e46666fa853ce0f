  <span class="p">}</span>;

  <span class="k">\node</span> at (<span class="p">$</span><span class="o">(</span><span class="nb">mat</span><span class="o">-</span><span class="m">1</span><span class="o">-</span><span class="m">1</span><span class="nb">.west</span><span class="o">)+(-</span><span class="m">16</span><span class="nb">pt,</span><span class="m">0</span><span class="o">)</span><span class="p">$</span>) <span class="p">{</span>Input: <span class="p">}</span>;
  <span class="k">\node</span> at (<span class="p">$</span><span class="o">(</span><span class="nb">mat</span><span class="o">-</span><span class="m">2</span><span class="o">-</span><span class="m">2</span><span class="nb">.west</span><span class="o">)+(-</span><span class="m">24</span><span class="nb">pt,</span><span class="m">0</span><span class="o">)</span><span class="p">$</span>) <span class="p">{</span>Hidden:<span class="p">}</span>;
  <span class="k">\node</span> at (<span class="p">$</span><span class="o">(</span><span class="nb">mat</span><span class="o">-</span><span class="m">3</span><span class="o">-</span><span class="m">2</span><span class="nb">.west</span><span class="o">)+(-</span><span class="m">24</span><span class="nb">pt,</span><span class="m">0</span><span class="o">)</span><span class="p">$</span>) <span class="p">{</span>Output:<span class="p">}</span>;

<span class="nt">\end{tikzpicture}</span>

