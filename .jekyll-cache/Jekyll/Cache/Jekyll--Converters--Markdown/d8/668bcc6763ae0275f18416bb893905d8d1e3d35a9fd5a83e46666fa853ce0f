<span class="nt">\begin{frame}</span>
<span class="nt">\begin{tikzpicture}</span>
[
plain/.style=<span class="p">{</span>
  draw=none,
  fill=none,
  <span class="p">}</span>,
  remember picture,
net/.style=<span class="p">{</span>
  matrix of nodes,
  nodes=<span class="p">{</span>
    draw,
    circle,
    inner sep=7.5pt
    <span class="p">}</span>,
  nodes in empty cells,
  column sep=-10.5pt,
  row sep=1.8cm
  <span class="p">}</span>
]
<span class="c">%\draw[help lines] (-3cm,-6cm) grid (6cm,3cm);</span>
<span class="k">\matrix</span><span class="na">[net]</span> (mat)
<span class="p">{</span>
              <span class="p">&amp;</span> |[plain]| <span class="p">&amp;</span>           <span class="p">&amp;</span> |[plain]|  <span class="p">&amp;</span>           <span class="p">&amp;</span> |[plain]| <span class="p">&amp;</span>           <span class="p">&amp;</span>  |[plain]|      <span class="p">&amp;</span>               <span class="k">\\</span>
    |[plain]| <span class="p">&amp;</span>           <span class="p">&amp;</span> |[plain]| <span class="p">&amp;</span>            <span class="p">&amp;</span> |[plain]| <span class="p">&amp;</span>           <span class="p">&amp;</span> |[plain]| <span class="p">&amp;</span>                 <span class="p">&amp;</span> |[plain]|     <span class="k">\\</span> 
